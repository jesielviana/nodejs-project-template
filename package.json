{
  "name": "modelo",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "lint": "eslint server.js src test",
    "debug": "babel-node --nolazy --inspect-brk=9229 ./server.js",
    "dev": "babel src -d dist",
    "test:integration": "NODE_ENV=test mocha  --exit --require @babel/register --require @babel/polyfill --require test/integration/helpers.js test/integration/**/*_spec.js",
    "test:unit": "NODE_ENV=test mocha --require @babel/register --require @babel/polyfill --require test/unit/helpers.js test/unit/**/*_spec.js",
    "test": "npm run test:unit && npm run test:integration",
    "build": "babel src -d functions",
    "postbuild": "cd functions && npm i -s",
    "serve": "npm run build && firebase serve",
    "deploy": "npm run build && firebase deploy"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^5.2.0",
    "rimraf": "^3.0.0",
    "sinon": "^7.2.2",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^8.6.0",
    "mongoose": "^5.7.3",
    "mongoose-unique-validator": "^2.0.2",
    "morgan": "^1.9.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.1"
  }
}
